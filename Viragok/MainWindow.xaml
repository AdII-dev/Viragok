<Window x:Class="Viragok.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Viragok"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="Viragok" Height="354" Margin="0,0,645,0" VerticalAlignment="Center" Width="145" SelectionChanged="Viragok_SelectionChanged"/>
        <Button Content="Törlés" HorizontalAlignment="Left" Margin="22,10,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>
        <Button Content="Másol" HorizontalAlignment="Left" Margin="78,10,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <ListBox x:Name="MasoltElemek" HorizontalAlignment="Left" Height="354" Margin="490,0,0,0" VerticalAlignment="Center" Width="300"/>
        <TextBox x:Name="Adatok" HorizontalAlignment="Left" Height="80" Margin="183,113,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="118"/>
        <Image x:Name="IMG" HorizontalAlignment="Left" Height="177" Margin="183,217,0,0" VerticalAlignment="Top" Width="217" Stretch="Fill"/>
    </Grid>
</Window>
